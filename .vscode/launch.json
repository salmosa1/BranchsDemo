{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Docker",
            "request": "launch",
            "type": "al",
            "environmentType": "Sandbox",
            "server": "http://Pronokal",
            "serverInstance": "BC",
            "authentication": "UserPassword",
            "breakOnError": "All",
            "launchBrowser": true,
            "enableLongRunningSqlStatements": true,
            "enableSqlInformationDebugger": true,
            "tenant": "default"
        },
        {
            "name": "Test SaaS",
            "type": "al",
            "request": "launch",
            "tenant": "pronokal.com",
            "environmentType": "Sandbox",
            "environmentName": "Pre",
            "breakOnError": true,
            "breakOnRecordWrite": false,
            "launchBrowser": true,
            "enableSqlInformationDebugger": true,
            "enableLongRunningSqlStatements": true,
            "longRunningSqlStatementsThreshold": 500,
            "numberOfSqlStatements": 10,
            "schemaUpdateMode": "Synchronize"
        },
        {
            "name": "Dev SaaS",
            "type": "al",
            "request": "launch",
            "tenant": "pronokal.com",
            "environmentType": "Sandbox",
            "environmentName": "Dev",
            "breakOnError": true,
            "breakOnRecordWrite": false,
            "launchBrowser": true,
            "enableSqlInformationDebugger": true,
            "enableLongRunningSqlStatements": true,
            "longRunningSqlStatementsThreshold": 500,
            "numberOfSqlStatements": 10,
            "schemaUpdateMode": "Synchronize"
        }
    ]
}